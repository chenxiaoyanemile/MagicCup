<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_record"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg_select"
    tools:context="com.example.sweetgirl.magiccup1.activity.MyRecordActivity">

    <include layout="@layout/my_record_toolbar"
        android:id="@+id/include" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/no_record"
        android:id="@+id/imageView2"
        android:layout_below="@+id/include"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="104dp"
        android:layout_marginEnd="104dp"
        android:layout_marginTop="105dp"
        tools:ignore="ContentDescription" />

</RelativeLayout>
